<div class="container well-lg">
  <h2>All Lineups:</h2>
  <ul class="list-group">
    <% @lineups.each do |lineup| %>
      <a href="/lineups/<%= lineup.id %>" class="list-group-item modal-sm">
      <%= lineup.name %>
      <br>
      <%= lineup.quarterback %>
      <%= lineup.runningback_one %>
      <%= lineup.runningback_two %>
      <%= lineup.widereceiver_one %>
      <%= lineup.widereceiver_two %>
      <%= lineup.widereceiver_three %>
      <%= lineup.tightend %>
      <%= lineup.flex %>
      <%= lineup.defense %>

      <% if lineup.user_id == current_user.id %>
        <p><strong>You own this lineup</strong></p>
      <% end %>

      </a>

    <br>
    <% end %>
  </ul>
</div>
